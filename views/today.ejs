<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Today</title>
  </head>
  <body>
    <% var d = new Date(); %> <% var n = d.getDay(); %>
    <h1><%=d.getDate()%>-<%=d.getMonth()%>-<%=d.getFullYear()%></h1>
    <% if(n==3) {%> <% result.forEach(res=>{ %> <% switch(res.freeSlot) { case
    '31' : %>
    <li>08:00 AM - 09:30 AM</li>
    <li><%=res.task%></li>

    <a href="/freeTimeScheduler/edit/<%=res.freeSlotId%>"
      ><button>Add/Edit Task</button></a
    >

    <% break; case '32' : %>
    <li>10:00 AM - 11:30 AM</li>
    <li><%=res.task%></li>

    <a href="/freeTimeScheduler/edit/<%=res.freeSlotId%>"
      ><button>Add/Edit Task</button></a
    >
    <% break; case '33' : %>
    <li>11:30 AM - 01:00 PM</li>
    <li><%=res.task%></li>

    <a href="/freeTimeScheduler/edit/<%=res.freeSlotId%>"
      ><button>Add/Edit Task</button></a
    >
    <% break; case '34' : %>
    <li>02:00 PM - 02:45 PM</li>
    <li><%=res.task%></li>

    <a href="/freeTimeScheduler/edit/<%=res.freeSlotId%>"
      ><button>Add/Edit Task</button></a
    >
    <% break; case '35' : %>
    <li>03:00 PM - 03:45 PM</li>
    <li><%=res.task%></li>

    <a href="/freeTimeScheduler/edit/<%=res.freeSlotId%>"
      ><button>Add/Edit Task</button></a
    >
    <% break; case '36' : %>
    <li>04:00 PM - 04:45 AM</li>
    <li><%=res.task%></li>

    <a href="/freeTimeScheduler/edit/<%=res.freeSlotId%>"
      ><button>Add/Edit Task</button></a
    >
    <% break; case '37' : %>
    <li>05:00 PM - 05:45 AM</li>
    <li><%=res.task%></li>

    <a href="/freeTimeScheduler/edit/<%=res.freeSlotId%>"
      ><button>Add/Edit Task</button></a
    >
    <% break; case '38' : %>
    <li>06:00 PM - 06:45 AM</li>
    <li><%=res.task%></li>

    <a href="/freeTimeScheduler/edit/<%=res.freeSlotId%>"
      ><button>Add/Edit Task</button></a
    >
    <% break; } %> <% }) %> <% } %>
  </body>
</html>
